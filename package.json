{
  "name": "GitEase",
  "version": "0.1.3",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "build:main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build": "npm run build:main && npm run build:renderer",
    "start:renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start:main-dev": "webpack --config webpack.main.config.js && electron ./dist/main.js",
    "start:dev": "cross-env START_HOT=1 npm run start:renderer-dev",
    "start:prestart": "npm run build",
    "start": "electron .",
    "lint": "tslint '{src,test,mocks}/**/*.{ts,tsx}' --project ./tsconfig.json",
    "test": "jest",
    "pretest:e2e": "npm run build",
    "test:e2e": "jest '(\\/test\\/e2e/).*'",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src}/**/*.{js,ts,tsx,json,css,scss,md}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ],
    "{src}/**/*.{js,ts,tsx}": [
      "tslint --project ./tsconfig.json --fix",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.+\\.spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(s?css|sass)$": "<rootDir>/mocks/styleMock.js"
    }
  },
  "build": {
    "productName": "GitEase",
    "appId": "org.nodoambiental.gitease",
    "extraResources": [
      "bin/",
      "appIcons/"
    ],
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "docs/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Robinfr/electron-react-typescript.git"
  },
  "author": {
    "name": "Agata Ordano",
    "email": "aordano@protonmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Robinfr/electron-react-typescript/issues"
  },
  "homepage": "https://github.com/Robinfr/electron-react-typescript",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@hot-loader/react-dom": "^16.11.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^24.0.24",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-hot-loader": "^4.1.1",
    "@types/react-redux": "^7.1.5",
    "@types/react-test-renderer": "^16.9.1",
    "@types/webdriverio": "^4.8.7",
    "@types/webpack-env": "^1.14.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.2.1",
    "css-loader": "^2.1.1",
    "electron": "^7.2.4",
    "electron-builder": "^20.44.4",
    "electron-devtools-installer": "^2.2.4",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.6.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.7.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.9.0",
    "lint-staged": "^8.2.1",
    "node-sass": "latest",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.18",
    "react-test-renderer": "^16.12.0",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^7.3.1",
    "simple-git-types": "^1.53.10",
    "source-map-loader": "^0.2.4",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typedoc": "^0.17.6",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^3.7.4",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@material-ui/icons": "^4.9.1",
    "@types/d3": "^5.7.2",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/feather-icons": "^4.7.0",
    "@types/js-yaml": "^3.12.3",
    "@types/react-d3-graph": "^2.3.1",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-tag-autocomplete": "^5.12.0",
    "@types/react-transition-group": "^4.2.4",
    "@types/sigmajs": "^1.0.27",
    "custom-electron-titlebar": "3.2.2-hotfix62",
    "d3": "^5.15.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "feather-icons": "^4.26.0",
    "full-icu": "^1.3.1",
    "i": "^0.3.6",
    "js-yaml": "^3.13.1",
    "react": "^16.12.0",
    "react-contextmenu": "^2.13.0",
    "react-d3-graph": "^2.3.0",
    "react-d3-tree": "^1.16.1",
    "react-dom": "^16.12.0",
    "react-feather": "^2.0.3",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-sigma": "^1.2.34",
    "react-tag-autocomplete": "^5.12.1",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "simple-git": "^1.129.0"
  }
}
